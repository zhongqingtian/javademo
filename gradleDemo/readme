// 闭包类型
// 定义闭包
def b1 = {
    println "hello b1"
}

// 定义这个方法，方法里面需要闭包类型的参数
def method1(Closure closure) { // Closure 表示参数是闭包类型
    closure()
}
// 调用方法method1
method1(b1)

